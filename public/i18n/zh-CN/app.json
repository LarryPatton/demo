    "naming": {
      "title": "资产命名规范",
      "description": "统一的资产命名约定，确保团队成员能快速识别资产类型和用途。",
      "format": {
        "title": "通用命名格式",
        "pattern": "[前缀]_[资产名称]_[变体]_[后缀]",
        "example": "T_CH_Player_Body_D.png"
      },
      "prefixes": {
        "title": "前缀规则（按资产类型）",
        "items": [
          { "prefix": "CH_", "type": "Character", "desc": "角色", "example": "CH_Player_Warrior" },
          { "prefix": "EN_", "type": "Environment", "desc": "场景", "example": "EN_Building_House01" },
          { "prefix": "PR_", "type": "Props", "desc": "道具", "example": "PR_Weapon_Sword" },
          { "prefix": "VFX_", "type": "Visual FX", "desc": "特效", "example": "VFX_Fire_Explosion" },
