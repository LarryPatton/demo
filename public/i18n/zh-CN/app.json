          { "prefix": "MAT_", "type": "Material", "desc": "材质", "example": "MAT_Metal_Gold" },
          { "prefix": "T_", "type": "Texture", "desc": "贴图", "example": "T_Player_D" },
          { "prefix": "SK_", "type": "Skeletal Mesh", "desc": "骨骼网格体", "example": "SK_Player_Body" },
          { "prefix": "SM_", "type": "Static Mesh", "desc": "静态网格体", "example": "SM_Rock_Large" },
          { "prefix": "A_", "type": "Animation", "desc": "动画", "example": "A_Player_Run" },
          { "prefix": "ABP_", "type": "Anim Blueprint", "desc": "动画蓝图", "example": "ABP_Player" },
