    "naming": {
      "title": "Asset Naming Convention",
      "description": "Unified naming conventions to help team members quickly identify asset types and purposes.",
      "format": {
        "title": "General Naming Format",
        "pattern": "[Prefix]_[AssetName]_[Variant]_[Suffix]",
        "example": "T_CH_Player_Body_D.png"
      },
      "prefixes": {
        "title": "Prefix Rules (By Asset Type)",
        "items": [
          { "prefix": "CH_", "type": "Character", "desc": "Character", "example": "CH_Player_Warrior" },
          { "prefix": "EN_", "type": "Environment", "desc": "Environment", "example": "EN_Building_House01" },
          { "prefix": "PR_", "type": "Props", "desc": "Props", "example": "PR_Weapon_Sword" },
          { "prefix": "VFX_", "type": "Visual FX", "desc": "Visual Effects", "example": "VFX_Fire_Explosion" },
          { "prefix": "UI_", "type": "User Interface", "desc": "UI Element", "example": "UI_Icon_Skill_Fireball" },
          { "prefix": "MAT_", "type": "Material", "desc": "Material", "example": "MAT_Metal_Gold" },
          { "prefix": "T_", "type": "Texture", "desc": "Texture", "example": "T_Player_D" },
          { "prefix": "SK_", "type": "Skeletal Mesh", "desc": "Skeletal Mesh", "example": "SK_Player_Body" },
          { "prefix": "SM_", "type": "Static Mesh", "desc": "Static Mesh", "example": "SM_Rock_Large" },
          { "prefix": "A_", "type": "Animation", "desc": "Animation", "example": "A_Player_Run" },
          { "prefix": "ABP_", "type": "Anim Blueprint", "desc": "Animation Blueprint", "example": "ABP_Player" },
          { "prefix": "SFX_", "type": "Sound FX", "desc": "Sound Effect", "example": "SFX_Explosion_01" }
        ]
      },
      "textureSuffixes": {
        "title": "Texture Suffix Rules",
        "items": [
          { "suffix": "_D", "type": "Diffuse/Albedo", "desc": "Diffuse/Base Color" },
          { "suffix": "_N", "type": "Normal", "desc": "Normal Map" },
          { "suffix": "_M", "type": "Metallic", "desc": "Metallic" },
          { "suffix": "_R", "type": "Roughness", "desc": "Roughness" },
          { "suffix": "_AO", "type": "Ambient Occlusion", "desc": "Ambient Occlusion" },
          { "suffix": "_E", "type": "Emissive", "desc": "Emissive" },
          { "suffix": "_H", "type": "Height", "desc": "Height Map" },
